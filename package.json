{
  "name": "pebble-transports",
  "author": "vboulaye",
  "version": "1.0.0",
  "keywords": [
    "pebble",
    "transilien"
  ],
  "private": true,
  "dependencies": {
    "pebble-clay": "^1.0.4",
    "pebblejs": "^1.0.0",
    "xmldom": "^0.1.27"
  },
  "pebble": {
    "displayName": "Mes Transports",
    "uuid": "e3ff1ec2-7372-4240-82c0-9f21690d67bb",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt",
      "chalk",
      "diorite"
    ],
    "watchapp": {
      "watchface": false
    },
    "messageKeys": [
      "BACKGROUND_COLOR",
      "TEXT_COLOR"
