        {
          "file": "images/menu_icon.png",
          "menuIcon": true,
          "name": "IMAGE_MENU_ICON",
          "type": "bitmap"
        },
        {
          "file": "images/logo_splash.png",
          "name": "IMAGE_LOGO_SPLASH",
          "type": "bitmap"
        },
        {
          "file": "images/tile_splash.png",
          "name": "IMAGE_TILE_SPLASH",
          "type": "bitmap"
        },
        {
          "file": "images/rerb.png",
          "name": "ICON_RER_B",
          "type": "bitmap"
        },
        {
          "file": "images/rerc.png",
          "name": "ICON_RER_C",
          "type": "bitmap"
        }
      ]
    }
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "mocha": "^3.5.0",
    "xmlhttprequest": "^1.8.0"
  },
  "scripts": {
    "test": "mocha src/pkjs/*.test.js"
  }
